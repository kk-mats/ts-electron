{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES6",
		"moduleResolution": "node",
		"lib": ["dom", "es2019"],
		"jsx": "react",
		"baseUrl": "./src",
		"esModuleInterop": true
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx",
		"webpack.config.*.ts"
	],
	"exclude": [
		"node_modules"
	]
}